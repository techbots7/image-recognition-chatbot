{
  "version": 2,
  "builds": [
    {
      "src": "*.html",
      "use": "@vercel/static"
    },
    {
      "src": "*.js",
      "use": "@vercel/static"
    },
    {
      "src": "*.css",
      "use": "@vercel/static"
    },
    {
      "src": "*.json",
      "use": "@vercel/static"
    },
    {
      "src": "lib/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "env": {
    "GEMINI_API_KEY": "your_gemini_api_key_here",
    "EMAILJS_SERVICE_ID": "service_dqganhw",
    "EMAILJS_TEMPLATE_ID": "template_wavm5up",
    "EMAILJS_USER_ID": "tXX17RsjHw9DKzwVu"
  }
} 