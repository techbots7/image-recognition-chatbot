{
  "version": 2,
  "builds": [
    {
      "src": "*.html",
      "use": "@vercel/static"
    },
    {
      "src": "lib/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/index.html"
    },
    {
      "src": "/login",
      "dest": "/login.html"
    },
    {
      "src": "/admin",
      "dest": "/admin.html"
    },
    {
      "src": "/chats",
      "dest": "/chats.html"
    },
    {
      "src": "/feedback",
      "dest": "/feedback.html"
    },
    {
      "src": "/statistics",
      "dest": "/statistics.html"
    },
    {
      "src": "/lib/(.*)",
      "dest": "/lib/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "env": {
    "GEMINI_API_KEY": "AIzaSyDX_UFZbUAp74JY0T9Jf7_Dq__5zaz_sPc",
    "EMAILJS_SERVICE_ID": "service_dqganhw",
    "EMAILJS_TEMPLATE_ID": "template_wavm5up",
    "EMAILJS_USER_ID": "tXX17RsjHw9DKzwVu"
  }
}