{
  "version": 2,
  "builds": [
    {
      "src": "*.html",
      "use": "@vercel/static"
    },
    {
      "src": "*.js",
      "use": "@vercel/static"
    },
    {
      "src": "*.css",
      "use": "@vercel/static"
    },
    {
      "src": "*.json",
      "use": "@vercel/static"
    },
    {
      "src": "lib/**/*",
      "use": "@vercel/static"
    },
    {
      "src": "my_model/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/my_model/(.*)",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable"
      },
      "continue": true
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "env": {
    "GEMINI_API_KEY": "AIzaSyDX_UFZbUAp74JY0T9Jf7_Dq__5zaz_sPc",
    "EMAILJS_SERVICE_ID": "service_dqganhw",
